{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AtlasNet bootstrap",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/DebugDocker/AtlasNet/AtlasNet",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "preLaunchTask": "make",
      "stopAtEntry": false,
      
    },
    {
      "name": "Debug Sample Game Client",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/DebugDocker/UnitTestsServer/UnitTestsServer",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "preLaunchTask": "make",
      "stopAtEntry": false,
      
      
    },
    
    {
      "name": "Debug Cartograph",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/DebugDocker/Cartograph/Cartograph",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "preLaunchTask": "make",
      "stopAtEntry": true,
      "logging": {
    "engineLogging": true,
    "trace": true,
    "traceResponse": true
}
      
    }
  ],
  "inputs": [
    {
      "id": "debugPort",
      "type": "promptString",
      "description": "Enter gdbserver port",
      "default": "1234"
    }
  ],

    "compounds": [
        {
            "name": "Debug Partion + Gameserver",
            "configurations": ["Attach To Partition", "Attach To SampleGame"]
        }
    ]
}